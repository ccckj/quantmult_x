port: 7890
allow-lan: true
mode: rule
log-level: info
unified-delay: true
global-client-fingerprint: chrome
ipv6: true
dns:
  enable: true
  listen: :53
  ipv6: true
  enhanced-mode: fake-ip
  fake-ip-range: 198.18.0.1/16
  default-nameserver:
    - 223.5.5.5
    - 8.8.8.8
  nameserver:
    - https://dns.alidns.com/dns-query
    - https://doh.pub/dns-query
  fallback:
    - https://1.0.0.1/dns-query
    - tls://dns.google
  fallback-filter:
    geoip: true
    geoip-code: CN
    ipcidr:
      - 240.0.0.0/4

proxies:
  - name: JP-Reality
    type: vless
    server: 74.226.240.109
    port: 8658
    uuid: 0081c712-3ce4-4251-b0ef-f5bf2b9f62fc
    network: tcp
    udp: true
    tls: true
    flow: xtls-rprx-vision
    servername: itunes.apple.com
    client-fingerprint: chrome
    reality-opts:
      public-key: LhoDq21PvIi3V3VcvyEWdpGakSV3jYJISHvVUXLgjSA
      short-id: 2eeed0176e0f0a7a

  - name: JP-Hysteria2
    type: hysteria2
    server: 74.226.240.109
    port: 8443
    #  up和down均不写或为0则使用BBR流控
    # up: "30 Mbps" # 若不写单位，默认为 Mbps
    # down: "200 Mbps" # 若不写单位，默认为 Mbps
    password: 9fa6864d564988af
    sni: bing.com
    skip-cert-verify: true
    alpn:
      - h3
  - name: JP-Vmess
    type: vmess
    server: speed.cloudflare.com
    port: 443
    uuid: 73ed5994-f8d3-49f1-862f-72e202ef2b24
    alterId: 0
    cipher: auto
    udp: true
    tls: true
    client-fingerprint: chrome
    skip-cert-verify: true
    servername: staff-northwest-yet-ste.trycloudflare.com
    network: ws
    ws-opts:
      path: 2c14eb4cbd5e
      headers:
        Host: staff-northwest-yet-ste.trycloudflare.com

proxy-groups:
  - name: 节点选择
    type: select
    proxies:
      - 自动选择
      - Reality
      - Hysteria2
      - Vmess
      - DIRECT

  - name: 自动选择
    type: url-test #选出延迟最低的机场节点
    proxies:
      - Reality
      - Hysteria2
      - Vmess
    url: "http://www.gstatic.com/generate_204"
    interval: 300
    tolerance: 50


rules:
    - GEOIP,LAN,DIRECT
    - GEOIP,CN,DIRECT
    - MATCH,节点选择
